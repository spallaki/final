{
  "name": "react-node-starter",
  "version": "1.0.0",
  "description": "an example for deploying a React + NodeJS app to Heroku",
  "main": "node_modules/expo/AppEntry.js",
  "private": true,
  "scripts": {
    "build": "webpack --config webpack.prod.config.js",
    "dev": "concurrently -k \"npm run frontend\" \"npm run backend\"",
    "frontend": "webpack -d --watch",
    "webpack": "webpack-dev-server --content-base build/",
    "backend": "nodemon server.js",
    "start": "NODE_ENV=production node server.js",
    "test": "mocha --compilers js:babel-core/register ./test/**/*.js --require ignore-styles",
    "migrateup": "db-migrate up -c 4",
    "migratedown": "db-migrate down -c 4"
  },
  "author": "",
  "license": "ISC",
  "dependencies": {
    "babel-core": "^6.24.1",
    "babel-loader": "^6.4.1",
    "babel-preset-es2015": "^6.24.1",
    "babel-preset-react": "^6.24.1",
    "body-parser": "^1.18.2",
    "cookie-parser": "^1.4.3",
    "css-loader": "^0.28.0",
    "db-migrate": "^0.10.2",
    "db-migrate-pg": "^0.2.5",
    "enzyme": "^2.8.1",
    "expect": "^1.20.2",
    "expo": "^23.0.0",
    "express": "^4.16.2",
    "express-session": "^1.15.6",
    "mocha": "^3.2.0",
    "morgan": "^1.9.0",
    "node-sass": "^4.6.1",
    "passport": "^0.4.0",
    "passport-local": "^1.0.0",
    "pg": "^7.4.0",
    "react": "^16.0.0",
    "react-dom": "^15.5.4",
    "react-native": "https://github.com/expo/react-native/archive/sdk-23.0.0.tar.gz",
    "react-redux": "^5.0.5",
    "redux": "^3.7.2",
    "redux-devtools": "^3.4.0",
    "redux-devtools-dock-monitor": "^1.1.2",
    "redux-devtools-log-monitor": "^1.3.0",
    "sass-loader": "^6.0.3",
    "style-loader": "^0.16.1",
    "webpack": "^2.3.3"
  },
  "devDependencies": {
    "babel-eslint": "^7.2.2",
    "concurrently": "^3.5.0",
    "eslint": "^3.19.0",
    "eslint-loader": "^1.9.0",
    "eslint-plugin-jsx-a11y": "^4.0.0",
    "eslint-plugin-react": "^6.10.3",
    "eslint-watch": "^3.1.0",
    "ignore-styles": "^5.0.1",
    "nodemon": "^1.11.0",
    "react-addons-test-utils": "^15.5.1",
    "react-test-renderer": "^15.5.4",
    "webpack-dev-middleware": "^1.10.1",
    "webpack-dev-server": "^2.9.1",
    "webpack-hot-middleware": "^2.18.0"
  }
}
